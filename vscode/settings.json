{
    "files.associations": {
        "**/templates/**/*.html": "django-html",
        "*.html": "html",
        "**/templates/*html": "django-html"
    },
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "emmet.triggerExpansionOnTab": true,  // optional, to trigger Emmet with Tab key
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    }
}
